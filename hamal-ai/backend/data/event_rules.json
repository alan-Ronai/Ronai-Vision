[
  {
    "_id": "default-armed-alert",
    "name": "התראת אדם חמוש",
    "description": "הפעלת מצב חירום כאשר מזוהה אדם חמוש",
    "enabled": false,
    "priority": 100,
    "conditions": {
      "operator": "AND",
      "items": [
        {
          "type": "object_detected",
          "params": {
            "objectType": "person",
            "minConfidence": 0.5
          }
        },
        {
          "type": "attribute_match",
          "params": {
            "attribute": "armed",
            "operator": "equals",
            "value": true
          }
        }
      ]
    },
    "pipeline": [
      {
        "type": "debounce",
        "params": {
          "cooldownMs": 30000,
          "key": "armed_alert_{cameraId}"
        }
      }
    ],
    "actions": [
      {
        "type": "log_event",
        "params": {
          "message": "CRITICAL: Armed person detected on camera {cameraId}",
          "level": "error"
        }
      },
      {
        "type": "system_alert",
        "params": {
          "severity": "critical",
          "title": "חדירה ודאית - אדם חמוש!",
          "message": "זוהה אדם חמוש במצלמה {cameraId}"
        }
      },
      {
        "type": "emergency_mode",
        "params": {
          "action": "start"
        }
      },
      {
        "type": "start_recording",
        "params": {
          "duration": 60,
          "preBuffer": 10
        }
      },
      {
        "type": "trigger_simulation",
        "params": {
          "simulationType": "phone_call",
          "delay": 2000
        }
      },
      {
        "type": "trigger_simulation",
        "params": {
          "simulationType": "drone_dispatch",
          "delay": 4000
        }
      }
    ],
    "tags": [
      "critical",
      "armed",
      "migrated"
    ],
    "migratedFrom": "alertService",
    "triggerCount": 11,
    "createdAt": "2025-01-01T00:00:00.000Z",
    "updatedAt": "2025-12-21T14:08:51.700Z",
    "lastTriggered": "2025-12-21T14:09:17.821Z"
  },
  {
    "_id": "default-multiple-people",
