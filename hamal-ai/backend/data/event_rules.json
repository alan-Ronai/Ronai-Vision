      {
        "type": "system_alert",
        "params": {
          "severity": "warning",
          "title": "זיהוי ריבוי אנשים",
          "message": "זוהו {personCount} אנשים במצלמה {cameraId}"
        }
      }
    ],
    "tags": [
      "warning",
